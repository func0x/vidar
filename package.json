		"generate-tags": "python3 ./GenerateTags.py"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "1.0.0",
		"@sveltejs/kit": "1.0.1",
		"@types/cookie": "^0.5.1",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.3"
	},
	"type": "module",
	"dependencies": {
		"@emotion/css": "^11.10.5",
		"@rollup/plugin-json": "^5.0.2",
