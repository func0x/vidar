		"generate-tags": "python3 ./GenerateTags.py"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.5",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "1.0.2",
		"@sveltejs/kit": "1.2.3",
		"@types/cookie": "^0.5.1",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.2",
		"typescript": "^4.9.4",
		"vite": "^4.0.4"
	},
	"type": "module",
	"dependencies": {
		"@emotion/css": "^11.11.2",
		"@rollup/plugin-json": "^5.0.2",
